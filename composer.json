{
    "name": "ymigval/laravel-indexnow",
    "type": "library",
    "description": "Laravel Service Library for notifying search engines about the latest content changes on their URLs using IndexNow.",
    "homepage": "https://github.com/ymigval/laravel-indexnow",
    "keywords": [
        "indexnow", 
        "search engine", 
        "website", 
        "seo", 
        "search", 
        "bing", 
        "yandex", 
        "laravel"
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Yordan",
            "email": "banatube@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ymigval\\LaravelIndexnow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ymigval\\LaravelIndexnow\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.9",
        "guzzlehttp/guzzle": "^7.7",
        "laravel/pint": "^1.13"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ymigval\\LaravelIndexnow\\Providers\\IndexNowServiceProvider"
            ],
            "aliases": {
                "Debugbar": "Ymigval\\LaravelIndexnow\\Facade\\IndexNow"
            }
        }
    }
}
